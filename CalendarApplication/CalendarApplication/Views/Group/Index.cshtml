@model CalendarApplication.Models.Group.GroupListModel

@{
    ViewBag.Title = "Index";
}

<h2>Groups</h2>


@using (Html.BeginForm())
{
    <table class="gridtable">
        @for (int i = 0; i < Model.GroupList.Count; i++)
        {
            <tr>
                <td>@Html.ActionLink(Model.GroupList[i].Name, "ViewGroup", new {groupId = Model.GroupList[i].ID})</td>
            </tr>
        }
    </table>
}